@{
    ViewBag.Title = "セルイチ管理機能";
    Layout = "~/Views/Shared/_t_menuLayout.cshtml";
}
@section styles{
    @Styles.Render("~/Content/tenfold_css_common")
}
    <div class="d-flex flex-column min-vh-100 bg-surface-secondary mt-18" id="wrapper">
        <form id="form1">
            <div id="sidebar-wrapper">
                <ul class="navbar-nav mb-md-4 p-3 mt-5 p-md-0">
                    <li>
                        <div class="ls-wider px-xl-3" href="#">
                            <input type="text" id="RealECD" name="RealECD" readonly="readonly" class="d-none" />
                            <input type="text" id="RealName" name="RealName" readonly="readonly" class="d-none" />
                        </div>
                    </li>
                    <li>
                        <div class="ls-wider px-xl-3" href="#">

                        </div>
                    </li>


                    <li>
                        <div class="text-center px-xl-3">

                            <div class="mb-3 text-start">
                                <label for=" " class="form-label">キーワード</label> <input type="text" class="form-control mb-2" id="Key" name="Key" placeholder="キーワード入力
                    ">
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text-center px-xl-3">
                            <div class="mb-3 text-start">
                                <div class="mb-3 text-start">
                                    <label for="" class="form-label">ステータス</label>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" name="Chk_MiShouri" id="Chk_MiShouri"
                                               checked> <label class="form-check-label" for="Chk_MiShouri">
                                            <i class="ms-1 ps-1 pe-1 rounded-circle bg-primary text-white fst-normal fst-normal">未</i><span class="font-semibold"> 未処理</span>
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" name="Chk_Shouri" id="Chk_Shouri"
                                               checked> <label class="form-check-label" for="Chk_Shouri">
                                            <i class="ms-1 ps-1 pe-1 rounded-circle bg-warning text-white fst-normal fst-normal">処</i><span class="font-semibold"> 処理中</span>
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" id="Chk_YouKakunin" name="Chk_YouKakunin"
                                               checked> <label class="form-check-label" for="Chk_YouKakunin">
                                            <i class="ms-1 ps-1 pe-1 rounded-circle bg-danger text-white fst-normal fst-normal">要</i><span class="font-semibold">要確認</span>
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" id="Chk_HouShuu" name="Chk_HouShuu"
                                               checked> <label class="form-check-label" for="Chk_HouShuu">
                                            <i class="ms-1 ps-1 pe-1 rounded-circle bg-dark text-white fst-normal fst-normal">保</i><span class="font-semibold">保 留</span>
                                        </label>
                                    </div>
                                    <div class="form-check">
                                        <input class="form-check-input" type="checkbox" value="" id="Chk_Soukyaku" name="Chk_Soukyaku"
                                               checked> <label class="form-check-label" for="Chk_Soukyaku">
                                            <i class="ms-1 ps-1 pe-1 rounded-circle bg-success text-white fst-normal fst-normal">保</i><span class="font-semibold">送客済</span>
                                        </label>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </li>
                    <li>
                        <div class="text-center px-xl-3">
                            <div class="mb-3 text-start">
                                <label for="ddlStaff" class="form-label">管理担当</label>

                                <select id="ddlStaff" name="ddlStaff" class="form-select mb-2">
                                    <option value="-1">全部表示</option>
                                    @foreach (Models.DropDownListItem item in ViewBag.TenstaffList)
                                    {
                                        <option class="js-selectable" value=@item.Value>@item.DisplayText</option>
                                    }
                                </select>

                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text-center px-xl-3">
                            <div class="mb-3 text-start">
                                <label for="ddlRange" class="form-label">範囲指定</label> <select id="ddlRange" name="ddlRange" class="form-select mb-2">
                                    <option>買取依頼日</option>
                                    <option>送客予定日</option>
                                </select>

                                <label for="StartDate" class="form-label">開始日</label>
                                <span class="cap-errormsg-right" id="errorStartDate"></span>
                                <input class="form-control mb-2" id="StartDate" name="StartDate" type="date" max="2999-12-31">
                                <label for="EndDate" class="form-label">終了日</label>
                                <span class="cap-errormsg-right" id="errorEndDate"></span>
                                <input class="form-control mb-2" id="EndDate" name="EndDate" type="date" max="2999-12-31">
                                <div class="row mb-3">
                                    <div class="col-6"> <button type="button" id="btnToday" name="btnToday" class="btn btn-primary w-100">今日</button></div>
                                    <div class="col-6"><button type="button" id="btnThisWeek" name="btnThisWeek" class="btn btn-primary w-100">今週</button></div>
                                </div>
                                <div class="row mb-3">
                                    <div class="col-6"><button type="button" id="btnThisMonth" name="btnThisMonth" class="btn btn-primary w-100">今月</button></div>
                                    <div class="col-6"><button type="button" id="btnLastMonth" name="btnLastMonth" class="btn btn-primary w-100">先月</button></div>
                                </div>
                            </div>
                        </div>
                    </li>

                    <li>
                        <div class="form-group ps-3 pe-3">
                            <button type="button" id="btnDisplay" class="btn btn-primary w-100 w-100 mb-3">表示</button>
                        </div>
                        <div class="form-group ps-3 pe-3">
                            <button type="button" id="btnCSV" class="btn btn-primary w-100 w-100 mb-3">CSVダウンロード</button>
                        </div>
                    </li>

                </ul>
                <div class="mt-auto pb-20"></div>
            </div>
        </form>
        <div class="toggle-button" name="left_side_NavigationBar_ShowHide_Icon">
            <a href="#menu-toggle" class="btn" id="menu-toggle">
                <span class="fa fa-angle-right glyphicon-chevron-right" aria-hidden="true"></span>
            </a>
        </div>

        <div class="d-flex flex-column min-h-screen bg-surface-secondary" id="page-content-wrapper">
            <main class="py-6 bg-surface-secondary">
                <div class="container-fluid">
                    @Html.Partial("t_assess_submenu")

                    <div class="card shadow border-0 mb-7" style="border-top-left-radius: unset;border-top-right-radius: unset;">
                        <div class="card-header d-md-inline-flex">
                            <h5 class="mb-0">処理待ち一覧</h5>
                            <span class="ps-md-5 text-muted text-sm" id="total_record_up"></span> <span style="color: red;" class="ps-md-5 text-sm" id="no_record"></span>
                        </div>
                        <div class="table-responsive">
                            <table id="tblPurchaseDetails" class="table table-hover table-nowrap">
                                <thead class="thead-light">
                                    <tr>
                                        <th scope="col" class="text-end text-decoration-underline text-end sort-by" ordercol-direction="asc" ordercol-index="7" style="width: 70px">No.</th>
                                        @*<th scope="col" class="text-decoration-underline "  style="width: 100px">NO</th>*@
                                        <th scope="col" style="width: 100px" class="text-decoration-underline sort-by" ordercol-index="1">ステータス</th>
                                        <th scope="col" style="width: 100px" class="text-decoration-underline sort-by" ordercol-index="1">査定ID</th>
                                        <th scope="col" style="width: 200px" class="text-decoration-underline sort-by" ordercol-index="2_6_1">物件（マンション、部屋番号）</th>
                                        <th scope="col" style="width: 150px" class="text-decoration-underline sort-by" ordercol-index="3_6_1">売主名</th>

                                        <th scope="col" style="width: 150px" class="text-decoration-underline sort-by" ordercol-index="4_6_1">不動産会社</th>
                                        <th scope="col" style="width: 150px" class="text-decoration-underline sort-by" ordercol-index="5_6_1">管理担当</th>
                                        <th scope="col" style="width: 150px" class="text-decoration-underline sort-by" ordercol-index="6_1">買取依頼日時</th>

                                        <th scope="col" style="width: 150px" class="text-decoration-underline sort-by" ordercol-index="7_1">経過時間</th>
                                        <th scope="col" style="width: 150px" class="text-decoration-underline sort-by" ordercol-index="8_6_1">	送客予定日</th>
                                        @*<th scope="col" style="width: auto" class=""></th>*@
                                    </tr>
                                </thead>
                                <tbody>
                                </tbody>
                            </table>
                        </div>
                        <div class="card-footer border-0 py-5">
                            <span class="text-muted text-sm" id="total_record"></span>
                        </div>
                    </div>
                </div>
            </main>
        </div> 
        <input type="hidden" id="UserCD" value="@ViewBag.UserCD" />
        @Html.Partial("t_assess_guide_popup")
        @Html.Partial("_t_footer")
        @Html.Partial("_ErrorPopup")
    </div>
    @section Scripts{
        @Scripts.Render("~/bundles/t_assess_guide")
    }

