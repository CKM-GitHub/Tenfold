@{
    ViewBag.Title = "簡易査定 - セルイチ";
}
@section styles{
    @Styles.Render("~/Content/css/a_index")
}
<form id="form1">
    <header class="text-center text-white cap-header">
        <div class="overlay"></div>
        <div class="container">
            <div class="siroeria">
                <div class="row">
                    <div class="col-xl-9 mx-auto position-relative pt-0 pt-md-3">
                        <h1 class="mb-md-5 cap-f30">マンション情報を入力してください。</h1>
                    </div>
                    <div class="col-md-10 col-lg-8 col-xl-8 col-12 mx-auto position-relative mt-0 mt-md-0">
                        <div class="row">
                            <div class="col-12 col-md-12">
                                <input class="d-none" type="text" name="SellerMansionID" id="SellerMansionID" value="@ViewBag.UpdateData.SellerMansionID" >
                                <input class="d-none" type="text" name="MansionCD" id="hdnMansionCD" value="@ViewBag.UpdateData.MansionCD">
                                <div>
                                    <input class="form-control w-100 typeahead" type="text" placeholder="マンション名" maxlength="50"
                                           name="MansionName" id="MansionName" value="@ViewBag.UpdateData.MansionName" autocomplete="off">
                                </div>
                                <div class="text-start">
                                    <span class="cap-errormsg-bottom" id="errorMansionName"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-10 col-lg-8 col-xl-8 col-12 mx-auto position-relative pt-md-0">
                        <div class="row pt-md-3">
                            <div class="col-12 col-md-3 pt-2 pt-lg-0">
                                <select class="form-select" name="PrefCD" id="PrefCD">
                                    <option selected value="">都道府県</option>
                                    @foreach (Models.DropDownListItem item in ViewBag.PrefDropDownListItems)
                                    {
                                        if (ViewBag.UpdateData.PrefCD == item.Value)
                                        {
                                            <option selected class="js-selectable" value="@item.Value">@item.DisplayText</option>
                                        }
                                        else
                                        {
                                            <option class="js-selectable" value="@item.Value">@item.DisplayText</option>
                                        }
                                    }
                                </select>
                                <div class="text-start">
                                    <span class="cap-errormsg-bottom" id="errorPrefCD"></span>
                                </div>
                            </div>
                            <div class="col-12 col-md-3 pt-2 pt-md-0">
                                <select class="form-select" name="CityCD" id="CityCD">
                                    <option selected value="">市区町村</option>
                                    @foreach (Models.DropDownListItem item in ViewBag.CityDropDownListItems)
                                    {
                                        if (ViewBag.UpdateData.CityCD == item.Value)
                                        {
                                            <option selected value="@item.Value">@item.DisplayText</option>
                                        }
                                        else
                                        {
                                            <option value="@item.Value">@item.DisplayText</option>
                                        }
                                    }
                                </select>
                                <div class="text-start">
                                    <span class="cap-errormsg-bottom" id="errorCityCD"></span>
                                </div>
                            </div>
                            <div class="col-12 col-md-3 pt-2 pt-md-0">
