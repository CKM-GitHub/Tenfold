@{
    ViewBag.Title = "査定 - セルイチ";
}
@section styles{
    @Styles.Render("~/Content/css/a_mypage_ahis")
}

<header class="text-center text-white masthead cap-header">

    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-xl-9 mx-auto position-relative">
                <h1 class="mb-2">マイページ</h1>
            </div>
        </div>
    </div>
</header>

<section class="clean-block clean-form dark h-100 cap-main" style="font-size: 14px !important;">
    <div class="container-fluid" id="wrapper">
        <div class="pt-5 pb-3">
            @Html.Partial("_MyPageMenu")
            <div class="card cap-form" id="page-content-wrapper">
                @*<form class="mx-auto cap-con-form card-body" id="form1">*@
                <form enctype="multipart/form-data" role="form" id="form1">
                    <h4 class="text-center pb-0 mb-0 pt-0 mt-0">
                       <strong>査定履歴</strong>
                    </h4>
                    @*<h4 class="text-center pb-0 mb-0 pt-0 mt-0"><strong>査定履歴</strong></h4>*@
                   <hr class=" pt-0 mt-0">
                    <div class="m-auto container">
                        <div class="row justify-content-center">
                            <div class="col-12">
                                <div class="card shadow">
                                    <div class="card-body">
                                        <div class="table-responsive"> 
                                            <table class="table table-striped table-hover sortMe" id="GetAssHistory">
                                                <thead>
                                                    <tr>
                                                        @*<th class="text-center text-decoration-underline text-nowrap sort-by cursor-pointer" ordercol-index="7" aria-label="Name : activate to sort column descending" data-type="number" style="width: 80px">No</th>*@
     <th class="text-center text-decoration-underline text-nowrap sort-by cursor-pointer" ordercol-direction="desc" ordercol-index="7" aria-label="Name : activate to sort column descending" style="width: 80px">No</th>

                                                        <th class="text-center text-decoration-underline text-nowrap sort-by cursor-pointer" style="width: 100px;">査定ID</th>
                                                        <th class="text-center text-decoration-underline text-nowrap sort-by cursor-pointer" ordercol-index="8_7" style="width: 80px;">ステータス</th>
                                                        <th class="text-nowrap text-decoration-underline sort-by cursor-pointer" ordercol-index="3_7" style="width: 120px;">物件(部屋)</th>
                                                        <th class="text-nowrap text-decoration-underline sort-by cursor-pointer" ordercol-index="4_7" style="width: 120px;">不動産会社</th>
                                                        <th class="text-nowrap text-decoration-underline text-end sort-by cursor-pointer" ordercol-index="5_7" style="width: 120px;">査定価格</th>
                                                        <th class="text-nowrap text-decoration-underline sort-by cursor-pointer" ordercol-index="6_7" style="width: 120px;">査定日時</th>
                                                        <th style="display:none">IDT</th>
                                                        <th style="display:none">StatusIndex</th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                    </div> 
                                    <div class="card-footer"> 

                                        <div class='pagination-container '>
                                            <nav>
                                                <ul id="pager" class="pagination justify-content-center">

                                                    <li class="page-item" data-page="prev">
                                                        <span> << <span class="sr-only">(current)</span></span>
                                                    </li> 
                                                    <li class="page-item" data-page="next" id="prev">
                                                        <span> >> <span class="sr-only">(current)</span></span>
                                                    </li>
                                                </ul>
                                            </nav>
                                        </div> 
                                    </div>
                                </div>
                            </div>
                        </div>
                </form>
            </div>
        </div>
    </div>
    <div class="modal fade" data-bs-backdrop="static" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">MansionName</h5>
                    <button type="button" class="btn-close d-none" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body text-center">
                    <input type="hidden" id="assessreqid" value="0" />
                    <div class="pt-5 pb-5 row">
                        <div class="col-6">
                            <button type="button" class="btn btn-success cap-btn-slg text-nowrap w-100 ps-1 pe-1 ps-sm-3" data-bs-toggle="modal"
                                    data-bs-target="#menu-seiyaku" id="btn_assess">
                                <i class="fa fa-history pe-1"
                                   aria-hidden="true"></i>査定結果
                            </button>
                        </div>
                        <div class="col-6">
                            <button type="button" class="btn btn-primary cap-btn-slg text-nowrap w-100 ps-1 pe-1 ps-sm-3" data-bs-toggle="modal"
                                    data-bs-target="#menu-jitai" id="btn_chat">
                                <i class="fa fa-comments-o pe-1"
                                   aria-hidden="true"></i>チャット
                            </button>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">戻る</button>
                </div>
            </div>
        </div>
    </div>
</section>
@section Scripts{
    @Scripts.Render("~/bundles/a_mypage_ahis")

}
 