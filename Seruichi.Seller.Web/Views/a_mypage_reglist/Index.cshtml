@{
    ViewBag.Title = "査定 - セルイチ";
}
@section styles{
    @Styles.Render("~/Content/css/a_mypage_reglist")
}
<header class="text-center text-white masthead cap-header">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-xl-9 mx-auto position-relative">
                <h1 class="mb-2">マイページ</h1>
            </div>
        </div>
    </div>
</header>


    <section class="clean-block clean-form dark h-100 cap-main">
        <div class="container-fluid" id="wrapper">
            <div class="pt-5 pb-3">
                @Html.Partial("_MyPageMenu")
                <div class="card cap-form">
                    <form action="index.html" method="post" enctype="multipart/form-data" role="form" id="form1">
                        <h4 style="text-align: center;padding-top: 50px;"><strong>登録物件</strong></h4>
                        <hr>
                        <div class="m-auto">
                            <div class="row justify-content-center">
                                <div class="col-12">
                                    <div class="card shadow">
                                        <div class="card-body">
                                            <div class="table-responsive">
                                                <table class="table table-striped table-hover" id="sellermansiontb">
                                                    <thead>
                                                        <tr>
                                                            <th class="text-center" style="width: 50px;">No</th>
                                                            <th class="text-start" style="width: 100px;">ステータス</th>
                                                            <th class="text-center" style="width: 80px;">査定</th>
                                                            <th class="text-start" style="width: 400px">物件（部屋）</th>
                                                            <th class="text-end" style="width: 180px;">登録日時</th>
                                                            <th class="text-end" style="width: 180px;">最終査定日時</th>
                                                            <th class="text-end" style="width: 150px;">査定額</th>
                                                            <th class="text-start" style="width: auto;">不動産会社</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
        <div class="modal fade" data-bs-keyboard="false" data-bs-backdrop="static" role="dialog" tabindex="-1"
             id="modal-sateitype">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="tourokuhouhouLabel">査定方法</h5>
                        <button type="button" class="btn-close d-none" data-bs-dismiss="modal"
                                aria-label="Close"></button>
                    </div>
                    <div class="modal-body text-center">
                        <input type="hidden" value="0" />
                        <div class="p-5">
                            <button type="button" class="btn btn-outline-primary btn-lg pt-3 pb-3"
                                    data-bs-toggle="modal" data-bs-target="#mansionmap" id="btn_assessment">
                                <i class="fa fa-search"
                                   aria-hidden="true"></i><span class="p-3">簡易査定</span>
                            </button>
                            <button type="button" class="btn btn-outline-success btn-lg pt-3 pb-3" id="btn_detailassessment">
                                <i class="fa fa-search-plus" aria-hidden="true"></i><span class="p-3">詳細査定</span>
                            </button>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-lg btn-secondary" data-bs-dismiss="modal">戻る</button>
                    </div>
                </div>
            </div>
        </div>

        <div class="modal fade" data-bs-keyboard="false" data-bs-backdrop="static" id="exampleModal" tabindex="-1" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">MansionName</h5>
                        <button type="button" class="btn-close d-none" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body text-center">
                        <div class="pt-5 pb-5 row">
                            <div class="col-6">
                                <button type="button" class="btn btn-success cap-btn-slg text-nowrap w-100 ps-1 pe-1 ps-sm-3" data-bs-toggle="modal"
                                        data-bs-target="#menu-seiyaku" id="btn_assess">
                                    <i class="fa fa-history pe-1"
                                       aria-hidden="true"></i>査定結果
                                </button>
                            </div>
                            <div class="col-6">
                                <button type="button" class="btn btn-primary cap-btn-slg text-nowrap w-100 ps-1 pe-1 ps-sm-3" data-bs-toggle="modal"
                                        data-bs-target="#menu-jitai" id="btn_chat">
                                    <i class="fa fa-comments-o pe-1"
                                       aria-hidden="true"></i>チャット
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">戻る</button>
                    </div>
                </div>
            </div>
        </div>

        <div class="modal fade" data-bs-keyboard="false" data-bs-backdrop="static" role="dialog" tabindex="-1"
             id="modal-1">
            <div class="modal-dialog modal-xl modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">簡易査定結果</h4><button type="button" class="btn-close d-none"
                                                                   data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <div>
                            <section class="kanekisatei pb-md-5 ">
                                <div class="form-container">
                                    <div class="row siroeria">
                                        <div class="col-md-8 col-12">
                                            <div class="justify-content-center align-items-center align-content-center align-self-center mx-auto fudousanjyouhou ">
                                                <h5 class="text-center">物件名<strong>物件名物件名物件名</strong></h5>
                                                <hr><span>住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所住所</span>
                                                <hr><span>
                                                    査定時間&nbsp; &nbsp; &nbsp; &nbsp;2022/01/01&nbsp;
                                                    12:34:56
                                                </span>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-12">
                                            <div>
                                                <div class="container kanekisateikekka">
                                                    <div class="row">
                                                        <div class="col-md-12 col-12">
                                                            <h6 class="text-start pt-3 pt-md-0">マンション査定</h6>
                                                            <div class="text-nowrap d-md-inline-flex justify-content-end w-100 pb-3">
                                                                <span>平均相場</span><span class="align-self-center cap-red-money">9,999</span><span>万円</span>
                                                            </div>
                                                            <hr class="d-none d-md-block">
                                                            <h6 class="text-start pt-3 pt-md-0">エリア査定</h6>
                                                            <div class="text-nowrap d-md-inline-flex justify-content-end w-100 pb-3">
                                                                <span>平均相場</span><span class="align-self-center cap-red-money">9,999</span><span>万円</span>
                                                            </div>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                    <div class="modal-footer justify-content-center text-center">
                        <button type="button"
                                class="btn btn-secondary btn-lg" data-bs-dismiss="modal">
                            戻る
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" data-bs-keyboard="false" data-bs-backdrop="static" role="dialog" tabindex="-1"
             id="zancheck">
            <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">利用可能詳細査定枠確認</h4><button type="button" class="btn-close d-none"
                                                                        data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <div>
                            <section class="kanekisatei p-5">
                                <div class="form-container">
                                    <div class="siroeria">
                                        <div class="justify-content-center mx-auto text-center">
                                            <h6 class="text-center pt-3 pt-md-0">利用可能詳細査定枠</h6>
                                            <div class="text-nowrap d-md-inline-flex justify-content-center w-100">
                                                <span>残り</span><span class="align-self-center cap-red-money text-danger font-weight-bold">1</span><span>件</span>
                                            </div>


                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                    <div class="modal-footer justify-content-center text-center">
                        <button type="button"
                                class="btn btn-secondary btn-lg" data-bs-dismiss="modal">
                            戻る
                        </button> <button class="btn btn-warning btn-lg"
                                          type="button">
                            増枠
                        </button>
                        <button class="btn btn-primary btn-lg" type="button" data-bs-target="#modal-phonenumcheck"
                                data-bs-toggle="modal">
                            次へ
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" data-bs-keyboard="false" data-bs-backdrop="static" role="dialog" tabindex="-1"
             id="modal-phonenumcheck">
            <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title">ご本人の確認</h4><button type="button" class="btn-close d-none"
                                                                   data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <section class="newsletter-subscribe" style="padding-top: 0;padding-left: 0;padding-bottom: 0;">
                            <div class="container">
                                <div class="">
                                    <div class="card">
                                        <div class="card-body">
                                            <form action="index.html" method="post" enctype="multipart/form-data"
                                                  role="form" style="border-width: 0px;width: 100%;max-width: 640px;">
                                                <div class="row">
                                                    <div class="col">
                                                        <p style="color: var(--bs-gray-dark);margin-bottom: 16px;">
                                                            本人確認のため、携帯電話のSMS（ショートメールサービス）を利用した認証確認を行います。
                                                        </p>
                                                        <div class="form-group mb-3">
                                                            <label class="form-label">携帯番号</label><input class="form-control" type="text" autocomplete="off"
                                                                                                         required="" placeholder="" name="phone"
                                                                                                         title="Debes escribir el nombre de una "
                                                                                                         style="max-width: 198px;" value="09012345678">
                                                        </div>
                                                        <p style="color: var(--bs-gray-dark);margin-bottom: 12px;font-size: 12px;">
                                                            電話番号は本人確認や不正利用防止のために利用します。&nbsp;&nbsp;
                                                        </p>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                    <div class="modal-footer justify-content-center text-center">
                        <button type="button"
                                class="btn btn-secondary btn-lg" data-bs-target="#zancheck"
                                data-bs-toggle="modal">
                            戻る
                        </button><button class="btn btn-primary btn-lg" type="button"
                                         data-bs-target="#modal-4" data-bs-toggle="modal">
                            携帯電話へ送信
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" data-bs-keyboard="false" data-bs-backdrop="static" role="dialog" tabindex="-1"
             id="modal-4">
            <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title"><strong>ご本人の確認</strong></h4><button type="button"
                                                                                    class="btn-close d-none" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <section class="newsletter-subscribe" style="padding-top: 0;padding-left: 0;padding-bottom: 0;">
                            <div class="container">
                                <div class="">
                                    <div class="card">
                                        <div class="card-body">
                                            <form action="index.html" method="post" enctype="multipart/form-data"
                                                  role="form" style="border-width: 0px;width: 100%;max-width: 640px;">
                                                <div class="row">
                                                    <div class="col">
                                                        <p style="color: var(--bs-gray-dark);margin-bottom: 16px;">
                                                            ***-****-5678にSMSで送りした6桁の認証番号を入力してください。
                                                        </p>
                                                        <div class="form-group mb-3">
                                                            <label class="form-label">認証番号</label><input class="form-control" type="text" autocomplete="off"
                                                                                                         required="" placeholder="" name="phone"
                                                                                                         title="Debes escribir el nombre de una "
                                                                                                         style="max-width: 198px;" value="123456">
                                                        </div>
                                                        <p style="color: var(--bs-gray-dark);margin-bottom: 12px;">
                                                            ⚠︎60秒経っても認証番号が届かない方へ<br>認証番号を再送することができます。
                                                        </p><button class="btn btn-primary" type="button">
                                                            認証番号を再送する
                                                        </button>
                                                    </div>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                    <div class="modal-footer justify-content-center text-center">
                        <button type="button"
                                class="btn btn-secondary btn-lg" data-bs-target="#modal-phonenumcheck"
                                data-bs-toggle="modal">
                            戻る
                        </button><button class="btn btn-primary btn-lg"
                                         type="button" data-bs-target="#timeover"
                                         data-bs-toggle="modal">
                            詳細査定結果表示
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal fade" data-bs-keyboard="false" data-bs-backdrop="static" role="dialog" tabindex="-1"
             id="timeover">
            <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="modal-title"><strong>ご本人の確認</strong></h4><button type="button"
                                                                                    class="btn-close d-none" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <section class="newsletter-subscribe" style="padding-top: 0;padding-left: 0;padding-bottom: 0;">
                            <div class="container">
                                <div class="">
                                    <div class="card">
                                        <div class="card-body">
                                            <div class="text-nowrap">

                                                <h6>  <span class="text-warning pe-2"><i class="fa fa-exclamation-circle" aria-hidden="true"></i></span>認証コード入力回数の上限を超えたため、ご利用いただけません。数時間後にもう一度お試しください。</h6>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    </div>
                    <div class="modal-footer justify-content-center text-center">
                        <button type="button"
                                class="btn btn-secondary btn-lg" data-bs-target="#modal-phonenumcheck"
                                data-bs-toggle="modal">
                            戻る
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </section>
    @section Scripts{
        @Scripts.Render("~/bundles/a_mypage_reglist")
    }


