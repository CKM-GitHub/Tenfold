@model Models.a_indexModel

@{
    ViewBag.Title = "簡易査定 - セルイチ";
}
@section styles{
    @Styles.Render("~/Content/css/a_index")
}

<form id="form1" novalidate>
    <header class="text-center text-white cap-header">
        <div class="overlay"></div>
        <div class="container">
            <div class="siroeria">
                <div class="row">
                    <div class="col-xl-9 mx-auto position-relative pt-0 pt-md-3">
                        <h1 class="mb-md-5 cap-f30">マンション情報を入力してください。</h1>
                    </div>
                    <div class="col-md-10 col-lg-8 col-xl-7 col-12 mx-auto position-relative mt-0 mt-md-0">
                        <div class="row">
                            <div class="col-6 col-md-3 cap-pt0-pl0 p-md-2 pe-md-0 pt-2">
                                <div class="d-inline-flex">
                                    <span class="align-self-center me-2 cap-yubin">〒</span>
                                    <input class="form-control cap-w-40 text-center" type="text" maxlength="3"
                                           name="ZipCode1" id="ZipCode1">
                                    <span class="align-self-center ps-1 pe-1">－</span>
                                    <input class="form-control w-50 text-center" type="text" maxlength="4"
                                           name="ZipCode2" id="ZipCode2">
                                </div>
                                <div class="text-start">
                                    <span class="cap-errormsg-bottom" id="errorZipCode"></span>
                                </div>
                            </div>
                            <div class="col-6 col-md-3 cap-pt0-pl0 p-md-2 pt-2 ps-md-3">
                                <select class="form-select" name="PrefCD" id="PrefCD">
                                    <option selected value="">都道府県</option>
                                    @foreach (Models.DropDownListItem item in ViewBag.PrefDropDownListItems)
                                    {
                                        <option class="js-selectable" value=@item.Value>@item.DisplayText</option>
                                    }
                                </select>
                                <div class="text-start">
                                    <span class="cap-errormsg-bottom" id="errorPrefCD"></span>
                                </div>
                            </div>
                            <div class="col-12 col-md-6 cap-pt0-pl0 p-md-2 pt-2 pb-2 ps-md-2">
                                <input class="d-none" type="text" name="MansionCD" id="hdnMansionCD">
                                <div>
                                    <input class="form-control w-100 typeahead" type="text" placeholder="マンション名" maxlength="50"
                                           name="MansionName" id="MansionName" autocomplete="off">
                                </div>
                                <div class="text-start">
                                    <span class="cap-errormsg-bottom" id="errorMansionName"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-10 col-lg-8 col-xl-7 col-12 mx-auto position-relative pt-md-0">
