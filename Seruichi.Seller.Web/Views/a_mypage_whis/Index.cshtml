@model Models.a_mypage_whisModel

@{
    ViewBag.Title = "利用履歴 - セルイチ";
}
@section styles{
    @Styles.Render("~/Content/css/a_mypage_whis")
}

<header class="text-center text-white masthead cap-header">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-xl-9 mx-auto position-relative">
                <h1 class="mb-2">利用履歴</h1>
            </div>
        </div>
    </div>
</header>
<div class="card cap-form">
    <form action="index.html" method="post" enctype="multipart/form-data" role="form">
        <h4 style="text-align: center;padding-top: 50px;"><strong>利用履歴</strong></h4>
        <hr>
        <div class="m-auto cap-fon-form-w600">
            <div class="row justify-content-center">
                <div class="col-12">
                    <div class="card shadow">
                        <div class="card-body">
                            <div class="table-responsive">
                                <table class="table table-striped table-hover">
                                    <thead>
                                        <tr>
                                            <th class="text-center" style="width: 50px;">No</th>
                                            <th class="text-center" style="width: 100px;">利用ID</th>
                                            <th class="text-center" style="width: 100px;">購入日時</th>
                                            <th class="text-end" style="width: 80px;">購入数</th>
                                            <th class="text-end" style="width: 100px;">購入金額</th>
                                            <th class="text-center" style="width: 100px;">結果</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td class="text-truncate text-center">1</td>
                                            <td class="text-truncate text-center">C00000000010</td>
                                            <td class="text-truncate text-center">2022/01/12 10:30:55</td>
                                            <td class="text-end">3</td>
                                            <td class="text-end">3,000円</td>
                                            <td class="text-center">成立</td>
                                        </tr>
                                       
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>
           


@section Scripts{
    @Scripts.Render("~/bundles/a_mypage_whis")
}