@model Models.a_mypage_whisModel
    @{
        ViewBag.Title = "利用履歴 - セルイチ";
    }
    @section styles{
        @Styles.Render("~/Content/css/a_mypage_whis")
    }

    <header class="text-center text-white masthead cap-header">

        <div class="overlay"></div>
        <div class="container">
            <div class="row">
                <div class="col-xl-9 mx-auto position-relative">
                    <h1 class="mb-2">マイページ</h1>
                </div>
            </div>
        </div>
    </header>
    <section class="clean-block clean-form dark h-100 cap-main">
        <div class="container-fluid" id="wrapper">
            <div class="pt-5 pb-3">
                @Html.Partial("_MyPageMenu")
                <div class="card cap-form">
                    <form action="index.html" method="post" enctype="multipart/form-data" role="form">
                        <h4 style="text-align: center;padding-top: 50px;"><strong>利用履歴</strong></h4>
                        <hr>
                        <div class="m-auto cap-fon-form-w600">
                            <div class="row justify-content-center">
                                <div class="col-12">
                                    <div class="card shadow">
                                        <div class="card-body">
                                            <div class="table-responsive">
                                                <table class="table table-striped table-hover" id="getSellerCD">
                                                    <thead>
                                                        <tr>
                                                            <th class="text-center" style="width: 50px;">No</th>
                                                            <th class="text-center" style="width: 100px;">利用ID</th>
                                                            <th class="text-center" style="width: 100px;">購入日時</th>
                                                            <th class="text-end" style="width: 80px;">購入数</th>
                                                            <th class="text-end" style="width: 100px;">購入金額</th>
                                                            <th class="text-center" style="width: 100px;">結果</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                </table>
                                            </div>
                                        </div>
                                        <div class="card-footer">

                                            <div class='pagination-container '>
                                                <nav>
                                                    <ul class="pagination justify-content-center">

                                                        <li class="page-item" data-page="prev">
                                                            <span> << <span class="sr-only">(current)</span></span>
                                                        </li>
                                                        <li class="page-item" data-page="next" id="prev">
                                                            <span> >> <span class="sr-only">(current)</span></span>
                                                        </li>
                                                    </ul>
                                                </nav>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>
    @section Scripts{
        @Scripts.Render("~/bundles/a_mypage_whis")
    }